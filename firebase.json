{
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/share/**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "/.well-known/apple-app-site-association",
                "headers": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ]
            }
        ]
    },
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "testmaker-f7dd2",
                    "appId": "1:651096216736:android:5bb5b8a41b82eb098725c5",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "ios": {
                "default": {
                    "projectId": "testmaker-f7dd2",
                    "appId": "1:651096216736:ios:c1a9e5befe7b3a868725c5",
                    "uploadDebugSymbols": false,
                    "fileOutput": "ios/Runner/GoogleService-Info.plist"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "testmaker-f7dd2",
                    "configurations": {
                        "android": "1:651096216736:android:5bb5b8a41b82eb098725c5",
                        "ios": "1:651096216736:ios:c1a9e5befe7b3a868725c5"
                    }
                }
            }
        }
    }
}